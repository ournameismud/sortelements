    },
    "extra": {
        "name": "Sort Elements",
        "handle": "sort-elements",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/ournameismud/sortelements/master/CHANGELOG.md",
